{
  "type": "module",
  "webbuilder": {
    "cloudflareSPARouter": true,
    "useTaggedTemplateReplacer": true,
    "uglify": true,
    "watch": [
      "src",
      "includes"
    ],
    "pugLocals": {
      "domain": "wynem.com"
    },
    "stylusPaths": [
      "includes/stylus"
    ],
    "forceHardReload": [
      "src/pages",
      "src/css",
      "includes/stylus"
    ]
  },
  "dependencies": {
    "webbuilder": "github:EvenTorset/WebBuilder"
  }
}
