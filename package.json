{
  "type": "module",
  "webbuilder": {
    "cloudflareSPARouter": true,
    "useTaggedTemplateReplacer": true,
    "uglify": true,
    "watch": [
      "src",
      "includes"
    ],
    "pugLocals": {
      "domain": "wynem.com"
    },
    "stylusPaths": [
      "includes/stylus"
    ],
    "forceHardReload": [
      "src/pages",
      "src/css"
    ]
  },
  "dependencies": {
    "webbuilder": "github:EvenTorset/WebBuilder#56280536070bc7e59c3b53e2fdcac26a3b0513c6"
  }
}
